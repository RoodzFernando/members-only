<%= provide(:title, "Edit | " + @user.name) %>

<%= render 'header' %>
    <%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
    <%= f.label :email %>
    <%= f.email_field :email, class: 'form-control' %>
    <%= f.submit "Edit" , class: 'btn btn-primary' %>
<% end %>